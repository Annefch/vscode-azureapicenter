{
    "name": "api-center-vscode-chat",
    "publisher": "ms-vscode",
    "displayName": "Copilot Chat API Center Agent",
    "description": "Sample chat agent extension",
    "repository": {
        "url": "https://github.com/microsoft/vscode-chat-agent-sample"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.83.0-20230923"
    },
    "extensionDependencies": [
        "github.copilot-chat"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "enabledApiProposals": [
        "chatSlashCommands",
        "chatAgents",
        "chatRequestAccess",
        "interactive",
        "readonlyMessage"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^20.5.9",
        "@types/vscode": "1.82.0",
        "eslint": "^7.22.0",
        "tslint": "^6.1.3",
        "typescript": "^4.0.3"
    }
}
